{
  "name": "sonar-web",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:circulohealth/sonar-web.git",
  "author": "Tony Duco <tony@circulohealth.com>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^2.10.8",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.19.0",
    "formik": "^2.2.6",
    "formik-material-ui": "^3.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.10",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@digitalroute/cz-conventional-changelog-for-jira": "^6.3.0",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "commitizen": "^4.2.3",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "lint": "tsc && eslint \"src/**/*\" --ext .ts,.tsx,.js,.jsx",
    "start": "react-scripts start",
    "build": "react-scripts build"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
    }
  },
  "pretiter": {
    "singleQuote": true,
    "trailingComma": "all",
    "allowParens": "avoid"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
